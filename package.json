{
  "name": "basic-adventure-gaming-system",
  "version": "0.0.0",
  "scripts": {
    "build": "npm-run-all -p  build:js build:css",
    "build:js": "rollup --config ./rollup.config.js",
    "build:css": "postcss src/index.css -o dist/bags.css",
    "start": "npm-run-all -p watch:js watch:css",
    "watch:js": "rollup --config ./rollup.config.js --watch",
    "watch:css": "postcss src/index.css -o dist/bags.css --watch --verbose --poll",
    "prepare": "husky install"
  },
  "author": "wyrmisis",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@babel/plugin-transform-private-methods": "^7.25.4",
    "@babel/preset-env": "^7.25.4",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@typhonjs-fvtt/eslint-config-foundry.js": "^0.8.0",
    "autoprefixer": "^10.4.7",
    "cssnano": "^7.0.6",
    "ember-template-lint-plugin-prettier": "^5.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.2.4",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "glob": "^11.0.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.1.0",
    "postcss-inherit": "^4.1.0",
    "postcss-nested": "^7.0.1",
    "postcss-reporter": "^7.0.5",
    "prettier": "^3.3.3",
    "prettier-plugin-ember-template-tag": "^2.0.4",
    "rollup": "^4.0.0",
    "rollup-plugin-copy-assets": "^2.0.3",
    "rollup-plugin-dotenv": "^0.5.1",
    "rollup-plugin-import-css": "^3.5.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-sourcemaps2": "^0.4.2",
    "stylelint": "^16.10.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.css": "stylelint --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
