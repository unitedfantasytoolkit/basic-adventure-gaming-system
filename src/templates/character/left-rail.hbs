  <article class="meters">
  </article>
    <div class="character-tiles">

      <div class="attack">

      </div>
      <div class="damage">

      </div>

      <div class="movement">

      </div>
    </div>

    <ul class="equipment">
      <li>
        <img src="https://placehold.co/32x32/000/fff" alt="" />
      </li>
    </ul>

    <ul class="favorites">
      <li>
        <img src="https://placehold.co/32x32/000/fff" alt="" />
      </li>
    </ul>
  </div>
